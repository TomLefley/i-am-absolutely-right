{
  "functions": {
    "app/api/increment/route.ts": {
      "maxDuration": 10
    },
    "app/api/stats/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/stats",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}